{
  "name": "athletics-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon backend/index.js",
    "client": "cd app && npm start",
    "install-all": "npm install && cd backend && npm install && cd ../app && npm install"
  },
  "dependencies": {
    "concurrently": "^9.0.0",
    "nodemon": "^3.1.0"
  }
}
